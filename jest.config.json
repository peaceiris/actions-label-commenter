{
  "silent": true,
  "verbose": true,
  "collectCoverage": true,
  "bail": 1,
  "clearMocks": true,
  "testEnvironment": "node",
  "testMatch": ["**/*.test.ts"],
  "transform": {
    "^.+\\.tsx?$": "ts-jest",
    "^.+\\.js$": "babel-jest"
  },
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "moduleNameMapper": {
    "^lodash-es$": "lodash"
  }
}
